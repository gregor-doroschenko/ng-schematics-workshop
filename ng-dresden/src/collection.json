{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-dresden-info": {
      "description": "Create simple text file with welcome.",
      "factory": "./ng-dresden-info/index#ngDresdenInfo"
    },
    "ng-dresden-param": {
      "description": "Create text file with meetup number and title",
      "factory": "./ng-dresden-param/index#ngDresdenParam",
      "schema": "./ng-dresden-param/schema.json"
    },
    "ng-dresden-component": {
      "description": "Create component",
      "factory": "./ng-dresden-component/index#ngDresdenComponent",
      "schema": "./ng-dresden-component/schema.json"
    }
  }
}
